{
  "name": "electron-react-parcel",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "parcel src/index.html",
    "clean": "rm -rf dist",
    "prebuild": "npm run clean",
    "build": "parcel build src/index.html",
    "serve-build": "npm run build && serve -s dist",
    "electron-prepare": "npm run build && bash ./electron/relative-html.sh",
    "electron-start": "npm run electron-prepare && electron ./electron/main.js",
    "electron-unpacked-build": "npm run electron-prepare && build --dir",
    "electron-build": "npm run electron-prepare && build"
  },

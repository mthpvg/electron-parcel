  "devDependencies": {
    "electron": "^3.0.2",
    "electron-builder": "^20.28.4",
    "parcel-bundler": "^1.10.1",
    "sass": "^1.14.0",
    "serve": "^10.0.1"
  },
  "dependencies": {
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
